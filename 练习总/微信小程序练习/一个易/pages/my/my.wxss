all-order
page view {
  overflow: hidden;
}
.header {
  /* display: flex; */
  justify-content: center;
  align-items: center;
  position: relative;
  height: 429rpx;
}
.userimg{
  width: 144rpx;
  height: 144rpx;
  display: block;
  border-radius: 50%;
  overflow: hidden;
  margin-bottom: 10rpx;

}
.header-banner {
position: absolute;
width: 100%;
height: 100%;
z-index: -1;
top: 0;
left: 0;
}
.head-name{
  text-align: center;
  position: absolute;
 top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  z-index: 10;
}
.title {
  position: absolute;
  z-index: 999;
  left: 0rpx;
  width: 100%;
  top: 50rpx;
  color: white;
  text-align: center;
}

.title-mobile {
  color: white;
  font-size: 42rpx;
}
.level {
  position: absolute;
  z-index: 999;
  width: 100%;
  left: 0rpx;
  bottom: 0rpx;
  height: 98rpx;
}
.level-time {
  position: absolute;
  z-index: 999;
  left: 0rpx;
  bottom: 0rpx;
  width: 100%;
  height: 98rpx;
  background-color: black;
  opacity: 0.15;
}

.level-image {
  position: absolute;
  z-index: 999;
  left: 20rpx;
  bottom: 25rpx;
  width: 34rpx;
  height: 48rpx;
}

.time-title {
  position: absolute;
  z-index: 999;
  bottom: 0rpx;
  left: 70rpx;
  width: 480rpx;
  line-height: 98rpx;
  height: 98rpx;
  color: white;
}

.access {
  position: absolute;
  z-index: 999;
  height: 26rpx;
  width: 14rpx;
  bottom: 36rpx;
  left: 690rpx;
}

.all-order {
  width: 100%;
  height: 100rpx;
  background-color: white;
  color: black;
  border-bottom: 1rpx solid lightgrey;
  background: #f81;
}

.order {
  height: 100rpx;
  line-height: 100rpx;
}

.all-order-title {
  margin-left: 20rpx;
  float: left;
}

.all-my-order {
  margin-right: 20rpx;
  float: right;
}

.op-order {
  background-color: white;
  overflow: hidden;
}

.sup-order {
  height: 144rpx;
  width: 33.33%;
  float: left;
}

.order-count {
  text-align: center;
  margin-top: 30rpx;
  color: black;
}

.order-title {
  margin-top: 20rpx;
  text-align: center;
  color: black;
}

.subitems {
  background-color: white;
  width: 100%;
  padding: 0 24rpx 0 0;
}

.action {
  border-bottom: 1rpx solid #eee;
  height: 88rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
}

.action image {
  width: 36rpx;
  height: 36rpx;
  margin: 26rpx 24rpx;
}

.action text {
  color: #353535;
  font-size: 32rpx;
  margin: 26rpx 0;
  height: 36rpx;
  line-height: 36rpx;
  overflow: hidden;
  display: inline-block;
}
.mylist{
  background-color: white;
  padding: 21rpx 30rpx ;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid #E6E6E6;
}
.mylist .mylisttitle{
 font-size: 30rpx;
 color: #333333;
}
.mylisticon{
  width: 84rpx;
  height: 84rpx;
  vertical-align: middle;
  margin-right: 30rpx;
}
.righticon{
  width: 17rpx;
  height: 30rpx;
}