}
.head {
  height: 80rpx;
  margin-top: 20rpx;
  margin-left: 75rpx;
  width: 600rpx;
  line-height: 40rpx;
}

.image {
    margin-top: 20rpx;
    height: 380rpx;
    width: 600rpx;
    margin-left: 75rpx;
}
.bottom {
  left: 0rpx;
  bottom: 0rpx;
  width: 100%;
  position: fixed;
  z-index: 100;
}
.bottomView{
  height: 40rpx;
  margin-top: 6rpx;
  margin-left: 75rpx;
  width: 600rpx;
  line-height: 40rpx;
}
