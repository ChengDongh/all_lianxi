  color: white;
  background-color: #f3244b;
  font-size: 28rpx;
  line-height: 88rpx;
  border-radius: 0rpx;
  width: 100%;
}
.input {
  margin-top: 40rpx;
  margin-left: 75rpx;
  width: 600rpx;
  height: 80rpx;
  border: 1rpx solid #ccc;
  border-radius:8rpx;
  background-color: white;
}
